  "dependencies": {
    "@mdx-js/mdx": "^1.0.20",
    "@mdx-js/react": "^1.0.20",
    "@mdx-js/tag": "^0.20.3",
    "dayjs": "^1.8.14",
    "gatsby": "^2.9.4",
    "gatsby-mdx": "^0.6.3",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-brotli": "^1.1.2",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-remark-autolink-headers": "^2.0.18",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-yuque-images": "^0.2.5",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-source-yuque": "^0.1.0",
    "gatsby-transformer-remark": "^2.4.0",
    "lodash": "^4.17.11",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "yarn clean && yarn build",
    "deploy": "yarn build && ./deploy.sh",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "rm -rf .cache public",
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ignore-pattern /functions --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.18.2",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}