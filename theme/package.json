{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "dayjs": "^1.10.4",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^2.0.0",
    "gatsby-plugin-catch-links": "^5.0.0",
    "gatsby-plugin-feed": "^5.0.0",
    "gatsby-plugin-layout": "^4.0.0",
    "gatsby-plugin-mdx": "^5.0.0",
    "gatsby-plugin-netlify": "^3.3.0",
    "gatsby-plugin-no-sourcemaps": "^5.0.0",
    "gatsby-plugin-page-creator": "^5.0.0",
    "gatsby-plugin-postcss": "^6.0.0",
    "gatsby-plugin-react-helmet": "^6.0.0",
    "gatsby-plugin-sharp": "^5.0.0",
    "gatsby-plugin-sitemap": "^6.0.0",
    "gatsby-remark-autolink-headers": "^6.0.0",
    "gatsby-remark-images": "^7.0.0",
    "gatsby-remark-prismjs": "^7.0.0",
    "gatsby-remark-yuque-images": "^1.2.0",
    "gatsby-source-filesystem": "^5.0.0",
    "gatsby-transformer-remark": "^6.0.0",
    "gatsby-transformer-yaml": "^5.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.2.13",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0",
    "preact-render-to-string": "^5.1.19",
    "prismjs": "^1.23.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "gatsby": "^5.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  },
  "peerDependencies": {
    "gatsby": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
